{
    "name": "pascalschwientek/excel",
    "description": "An eloquent way of importing and exporting Excel and CSV in Laravel 4 with the power of PHPExcel",
    "license": "LGPL",
    "keywords": ["laravel", "phpexcel", "excel", "csv", "export", "import", "batch"],
    "require": {
        "php": ">=5.4.0",
        "phpoffice/phpexcel": "~1.8.0",
        "phpseclib/phpseclib": ">=0.3.7",
        "illuminate/cache": "~5.0",
        "illuminate/config": "~5.0",
        "illuminate/filesystem": "~5.0",
        "illuminate/http": "~5.0",
        "illuminate/support": "~5.0",
        "illuminate/routing": "~5.0",
        "illuminate/view": "~5.0",
        "nesbot/carbon": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "mockery/mockery": "~0.9",
        "orchestra/testbench": "~2.2.0"
    },
    "autoload": {
        "classmap": [
            "src/Maatwebsite/Excel",
            "tests/TestCase.php"
        ],
        "psr-0": {
            "Maatwebsite\\Excel\\": "src/"
        }
    }
}
